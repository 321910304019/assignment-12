In [8]:

Tuple=(1,2,3,4,5,7,8,9)
 print(Tuple[2:]) 
print(Tuple[1:4])
 print(Tuple[-6:-1]) 

(3,4,5,7,8,9) 
(2, 3, 4) 
(3, 4, 5,7,8) 

